{
  "name": "storyblok-nextjs",
  "version": "1.0.0",
  "description": "Next.js integration for Storyblok",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "storyblok-rich-text-react-renderer": "^2.9.2",
    "type-assurance": "^1.3.0"
  },
  "peerDependencies": {
    "@storyblok/react": ">=2",
    "next": ">=13",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@jest/types": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/react": "^18.2.0",
    "typescript": "^5.0.0",
    "vitest": "^3.0.8"
  },
  "keywords": [
    "storyblok",
    "nextjs",
    "react"
  ],
  "license": "MIT"
}
